Cell[TextData[{
 "When we first did two coupled harmonic oscillators, we had two positions, \
two velocities, and two formulas for acceleration:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"94f8a658-444b-4e05-9e90-6f66ff91b477"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["x", "1"], "*"], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "1"], "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       SubscriptBox["v", "1"], "(", 
       SubscriptBox["t", "i"], ")"}], "\[CenterDot]", "\[VeryThinSpace]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"1c603f4c-077e-4176-8c1a-bfc89ea37dd8"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["x", "2"], "*"], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "2"], "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       SubscriptBox["v", "2"], "(", 
       SubscriptBox["t", "i"], ")"}], "\[CenterDot]", "\[VeryThinSpace]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"6c5ed670-8a21-4530-bc8a-ff41940f6fe4"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["v", "1"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["v", "1"], "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["a", "1"], "(", 
        RowBox[{
         SuperscriptBox[
          SubscriptBox["x", "1"], "*"], ",", 
         SuperscriptBox[
          SubscriptBox["x", "2"], "*"]}], ")"}], "\[CenterDot]", 
       "\[CapitalDelta]"}], "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"9c5b62dc-eff0-4ab9-b449-fc42c31b086a"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["v", "2"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["v", "2"], "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["a", "2"], "(", 
        RowBox[{
         SuperscriptBox[
          SubscriptBox["x", "1"], "*"], ",", 
         SuperscriptBox[
          SubscriptBox["x", "2"], "*"]}], ")"}], "\[CenterDot]", 
       "\[CapitalDelta]"}], "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"8ae8dc65-fbf6-4f7f-8a0d-73185144fca2"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["x", "1"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "1"], "(", 
      SubscriptBox["t", "i"], ")"}], "+", " ", 
     RowBox[{
      FormBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["v", "1"], "(", 
          SubscriptBox["t", "i"], ")"}], "+", 
         RowBox[{
          SubscriptBox["v", "1"], "(", 
          SubscriptBox["t", 
           RowBox[{"i", "+", "1"}]], ")"}]}], ")"}],
       TraditionalForm], 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"175ef99a-d664-4b6a-a133-364c66b8aeda"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["x", "2"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "2"], "(", 
      SubscriptBox["t", "i"], ")"}], "+", " ", 
     RowBox[{
      FormBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["v", "2"], "(", 
          SubscriptBox["t", "i"], ")"}], "+", 
         RowBox[{
          SubscriptBox["v", "2"], "(", 
          SubscriptBox["t", 
           RowBox[{"i", "+", "1"}]], ")"}]}], ")"}],
       TraditionalForm], 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"9f4b4c6b-9a82-4d3a-be4a-5eb2cd8fb343"]
}], "Text",
 CellChangeTimes->{{3.948639418999289*^9, 3.948639679290537*^9}, {
