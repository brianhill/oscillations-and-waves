Cell[TextData[{
 "Here is a recap of the second-order Runge-Kutta formula for the pendulum \
formula, with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Lambda]", "=", 
    FractionBox["1", "2"]}], TraditionalForm]],ExpressionUUID->
  "e8a706a1-c05e-45ea-98b4-63b51843ebb3"],
 ", and removing the possibility of a damping force and removing explicit \
dependence on time. Those three assumptions make the equations a little \
easier:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"b94625b0-8f48-49f9-b64c-6cf2b20dd691"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Theta]", "*"], "=", 
    RowBox[{
     RowBox[{"\[Theta]", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"\[Omega]", "(", 
       SubscriptBox["t", "i"], ")"}], "\[CenterDot]", "\[VeryThinSpace]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"4a13b651-292f-4035-88e3-1eb54bf6ee47"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Omega]", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"\[Omega]", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Alpha]", "(", 
        SuperscriptBox["\[Theta]", "*"], ")"}], "\[CenterDot]", 
       "\[CapitalDelta]"}], "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"b038f15d-b88f-4266-b13c-f601a340a3b7"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Theta]", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"\[Theta]", "(", 
      SubscriptBox["t", "i"], ")"}], "+", " ", 
     RowBox[{
      FormBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Omega]", "(", 
          SubscriptBox["t", "i"], ")"}], "+", 
         RowBox[{"\[Omega]", "(", 
          SubscriptBox["t", 
           RowBox[{"i", "+", "1"}]], ")"}]}], ")"}],
       TraditionalForm], 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"b6d5ab69-11db-45a8-b341-7538f4a1fb5f"]
}], "Text",
 CellChangeTimes->{{3.947895083588633*^9, 3.947895141759205*^9}, {
