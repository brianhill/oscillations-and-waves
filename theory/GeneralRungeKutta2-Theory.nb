Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["t", "*"], "=", 
    RowBox[{"t", "+", 
     RowBox[{
     "\[Alpha]", "\[VeryThinSpace]", "\[CapitalDelta]", "\[VeryThinSpace]", 
      "t"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "263d57b5-d981-4dc5-85a9-5e15ce66d7fb"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "*"], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"v", "(", 
        SubscriptBox["t", "i"], ")"}], "\[CenterDot]", "\[Alpha]"}], "\[VeryThinSpace]",
       "\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"689833de-4b98-482d-9747-a7923ac7ecd6"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["v", "*"], "=", 
    RowBox[{
     RowBox[{"v", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"a", "(", 
        RowBox[{
         SubscriptBox["t", "i"], ",", 
         RowBox[{"x", "(", 
          SubscriptBox["t", "i"], ")"}], ",", 
         RowBox[{"v", "(", 
          SubscriptBox["t", "i"], ")"}]}], ")"}], "\[CenterDot]", 
       "\[Alpha]"}], "\[VeryThinSpace]", "\[CapitalDelta]", "\[VeryThinSpace]",
       "t"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7f216591-a852-4694-b2de-fd8d72e4de4c"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"26876951-c52d-4886-a15e-ebb0581fdbdf"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"v", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", 
            FractionBox["1", 
             RowBox[{"2", "\[Alpha]"}]]}], ")"}], 
          RowBox[{"a", "(", 
           RowBox[{
            SubscriptBox["t", "i"], ",", 
            RowBox[{"x", "(", 
             SubscriptBox["t", "i"], ")"}], ",", 
            RowBox[{"v", "(", 
             SubscriptBox["t", "i"], ")"}]}], ")"}]}], "+", 
         RowBox[{
          FractionBox["1", 
           RowBox[{"2", "\[Alpha]"}]], 
          RowBox[{"a", "(", 
           RowBox[{
            SuperscriptBox["t", "*"], ",", 
            SuperscriptBox["x", "*"], ",", 
            SuperscriptBox["v", "*"]}], ")"}]}]}], ")"}], "\[CenterDot]", 
       "\[CapitalDelta]"}], "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"3d42a889-056b-4806-8b60-7a040c5b3a6f"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", " ", 
     RowBox[{
      FormBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"v", "(", 
          SubscriptBox["t", "i"], ")"}], "+", 
         RowBox[{"v", "(", 
          SubscriptBox["t", 
           RowBox[{"i", "+", "1"}]], ")"}]}], ")"}],
       TraditionalForm], 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"a5d966a4-9344-4410-9fd2-e5a481ecc5f0"],
 "\n\nAfter doing the sanity checks and comparing the new and \
previously-contemplated formulas, I have opted to restore the trapezoid for \
",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "(", 
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "26e7e4bf-6ea5-4c69-be00-479b46ab8d93"],
 ", even though it is not quite what is specified in Eq. 231b of Butcher, ",
 StyleBox["Numerical Methods for Ordinary Differential Equations,",
  FontSlant->"Italic"],
 " Wiley, 2003."
}], "Text",
 InitializationCell->True,
