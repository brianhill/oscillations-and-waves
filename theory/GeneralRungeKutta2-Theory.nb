Cell[TextData[{
 "This is the version of second-order Runge-Kutta that we derived and used on \
January 29:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"05883b0a-287d-43f2-ab32-75886fd4efc4"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["x", "*"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"v", "(", 
       SubscriptBox["t", "i"], ")"}], "\[CapitalDelta]", "\[VeryThinSpace]", 
      "t"}]}]}], TraditionalForm]],ExpressionUUID->
  "3e8e067d-9fa5-4a16-ac4c-f5d180e48b0d"],
 "     (using ",
 Cell[BoxData[
  FormBox[
   RowBox[{"v", "(", 
    SubscriptBox["t", "i"], ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "96150dca-7dc8-4209-a9f9-aaff84779983"],
 " to extrapolate all the way to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["t", 
    RowBox[{"i", "+", "1"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7fc8efb4-df27-4a07-9a85-fe83a8cdd546"],
 ")\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"v", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"a", "(", 
         RowBox[{"x", "(", 
          SubscriptBox["t", "i"], ")"}], ")"}], "+", 
        RowBox[{"a", "(", 
         RowBox[{
          SuperscriptBox["x", "*"], "(", 
          SubscriptBox["t", 
           RowBox[{"i", "+", "1"}]], ")"}], ")"}]}], ")"}], "\[CenterDot]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"516f24f7-501b-4291-a69a-f95c373050a8"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"v", "(", 
         SubscriptBox["t", "i"], ")"}], "+", 
        RowBox[{"v", "(", 
         SubscriptBox["t", 
          RowBox[{"i", "+", "1"}]], ")"}]}], ")"}], "\[CenterDot]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"25e92c4d-1498-448b-969b-0ddcbb6d8925"],
 "\n\nThe intermediate quantity ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "*"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "30e6ff30-0f52-4438-bd88-48de2be50785"],
 " is computed and used, but it is not recorded as part of the results of the \
current time step or handed off to the next time step. It is computed, used, \
and then forgotten."
}], "Text",
 CellChangeTimes->{{3.944067896816962*^9, 3.94406793882053*^9}, {
