Cell[TextData[{
 "With ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["\[Omega]", "0"], "2"], "=", 
    FractionBox[
     RowBox[{"\[VeryThinSpace]", "g"}], "l"]}], TraditionalForm]],
  ExpressionUUID->"5e012c79-48a3-4ec6-8af7-b30159fa6dac"],
