Cell[TextData[{
 "We were motivated to try this system of equations for doing updates when ",
 Cell[BoxData[
  FormBox["F", TraditionalForm]],ExpressionUUID->
  "2a5d4111-2586-4120-a600-3529e815e17e"],
 " or ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],ExpressionUUID->
  "88c0dd55-7e35-4ee0-ba77-938783df45b3"],
 " depended on time indirectly via the position ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "a461f658-dc30-45d9-a26a-5b086469fd1b"],
 ":\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"b1884bba-c06b-4e20-980d-10f2fb9be0c2"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"v", "(", 
          SubscriptBox["t", "i"], ")"}], "+", 
         RowBox[{"v", "(", 
          SubscriptBox["t", 
           RowBox[{"i", "+", "1"}]], ")"}]}], "2"], "\[CenterDot]", 
       "\[CapitalDelta]"}], "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"177dbd66-1f37-4cbd-9e4a-b5b02f02883e"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"v", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"a", "(", 
          RowBox[{"x", "(", 
           SubscriptBox["t", "i"], ")"}], ")"}], "+", 
         RowBox[{"a", "(", 
          RowBox[{"x", "(", 
           SubscriptBox["t", 
            RowBox[{"i", "+", "1"}]], ")"}], ")"}]}], "2"], "\[CenterDot]", 
       "\[CapitalDelta]"}], "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"e0a2c613-0a37-426e-9b7a-035cd1561be3"],
 "\n\n",
 StyleBox["We realized that this system suffered a chicken-and-egg problem.",
  FontSlant->"Italic"],
 "\n\nWe decided to try this system of equations instead:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"05883b0a-287d-43f2-ab32-75886fd4efc4"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["x", "*"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"v", "(", 
       SubscriptBox["t", "i"], ")"}], "\[CapitalDelta]", "\[VeryThinSpace]", 
      "t"}]}]}], TraditionalForm]],ExpressionUUID->
  "3e8e067d-9fa5-4a16-ac4c-f5d180e48b0d"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"v", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"a", "(", 
         RowBox[{"x", "(", 
          SubscriptBox["t", "i"], ")"}], ")"}], "+", 
        RowBox[{"a", "(", 
         RowBox[{
          SuperscriptBox["x", "*"], "(", 
          SubscriptBox["t", 
           RowBox[{"i", "+", "1"}]], ")"}], ")"}]}], ")"}], "\[CenterDot]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"516f24f7-501b-4291-a69a-f95c373050a8"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"v", "(", 
         SubscriptBox["t", "i"], ")"}], "+", 
        RowBox[{"v", "(", 
         SubscriptBox["t", 
          RowBox[{"i", "+", "1"}]], ")"}]}], ")"}], "\[CenterDot]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"25e92c4d-1498-448b-969b-0ddcbb6d8925"],
 "\n\nThis system of equations is known as Second-Order Runge-Kutta, and it \
is what we are going to use next in our Mathematica notebooks."
}], "Text",
 CellChangeTimes->{{3.944067896816962*^9, 3.94406793882053*^9}, {
