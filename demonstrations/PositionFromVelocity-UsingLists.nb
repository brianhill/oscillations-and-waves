Cell[TextData[{
 "We still have our core formulas for moving ahead in time by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ce493e01-05d9-46e3-99fa-a831166fd560"],
 ":\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", "2"], "=", 
    RowBox[{
     SubscriptBox["t", "1"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5ed832ee-7a81-4b31-a1f6-db6e3242b45f"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", "2"], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "1"], ")"}], "+", 
     RowBox[{
      RowBox[{"v", "(", 
       RowBox[{
        SubscriptBox["t", "1"], "+", 
        FractionBox[
         RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}], ")"}], 
      "*", "\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"f272d1e8-e46f-4ab1-93d1-7a713218455d"]
}], "Text",
