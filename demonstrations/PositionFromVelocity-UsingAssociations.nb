Cell[BoxData[
 RowBox[{"NestList", "[", 
  RowBox[{"move", ",", 
   RowBox[{"<|", 
    RowBox[{
     RowBox[{"\"\<time\>\"", "->", "2.0"}], ",", " ", 
     RowBox[{"\"\<position\>\"", "->", 
