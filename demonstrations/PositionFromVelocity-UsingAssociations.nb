       RowBox[{"\"\<time\>\"", "->", "2.0"}], ",", " ", 
       RowBox[{"\"\<position\>\"", "->", 
        RowBox[{"-", "2.0"}]}]}], "|>"}], ",", "150"}], "]"}]}], 
  ";"}]], "Input",
