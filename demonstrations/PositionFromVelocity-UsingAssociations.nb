Cell[BoxData[
 RowBox[{"stateAsAList", " ", "=", 
  RowBox[{"{", 
   RowBox[{"3600", ",", "10000"}], "}"}]}]], "Input",
 CellChangeTimes->{{3.944259814219489*^9, 3.9442598508949623`*^9}, {
