Cell[TextData[{
 "We will use the formulas we derived in the \[OpenCurlyDoubleQuote]Position \
from Velocity \[LongDash] Theory\[CloseCurlyDoubleQuote] notebook:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", "2"], "=", 
    RowBox[{
     SubscriptBox["t", "1"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "08ddfff7-3cbb-40d6-8128-d737254dc6d3"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", "2"], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "1"], ")"}], "+", 
     RowBox[{
      RowBox[{"v", "(", 
       RowBox[{
        SubscriptBox["t", "1"], "+", 
        FractionBox[
         RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}], ")"}], 
      "*", "\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"f272d1e8-e46f-4ab1-93d1-7a713218455d"]
}], "Text",
 CellChangeTimes->{{3.944067896816962*^9, 3.94406793882053*^9}, {
