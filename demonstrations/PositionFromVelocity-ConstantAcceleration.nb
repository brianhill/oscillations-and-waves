Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"v", "[", "t_", "]"}], ":=", 
   RowBox[{"6", "t"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
