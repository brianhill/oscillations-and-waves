Cell[BoxData[{
 RowBox[{
  RowBox[{"times", " ", "=", " ", 
   RowBox[{"Range", "[", 
    RowBox[{"tInitial", ",", "tFinal", ",", "deltaT"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"midpointTimes", "=", 
    RowBox[{
     RowBox[{"Drop", "[", 
      RowBox[{"times", ",", 
       RowBox[{"-", "1"}]}], "]"}], "+", "0.2"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "drop", " ", "the", " ", "last", " ", "element", " ", "which", " ", "is", 
    " ", "6.2"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"midpointVelocities", "=", 
    RowBox[{"6", "midpointTimes"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "our", " ", "velocity", " ", "function", " ", "is", " ", "just", " ", "6", 
    " ", 
    StyleBox["t",
     FontSlant->"Italic"]}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"displacements", " ", "=", " ", 
    RowBox[{"midpointVelocities", " ", "deltaT"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "the", " ", "product", " ", "of", " ", "velocity", " ", "and", " ", 
    "deltaT", " ", "gives", " ", "displacement"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"positions", " ", "=", " ", 
    RowBox[{"Accumulate", "[", "displacements", "]"}]}], ";"}], 
  " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"positions", " ", "=", " ", 
    RowBox[{"Prepend", "[", 
     RowBox[{"positions", ",", "0.0"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Accumulate", " ", 
     RowBox[{"didn", "'"}], "t", " ", "give", " ", "us", " ", "the", " ", 
     "initial", " ", "position", " ", "of", " ", "0.0"}], ",", " ", 
    RowBox[{
    "so", " ", "we", " ", "need", " ", "to", " ", "explicitly", " ", 
     "prepend", " ", "it"}]}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"orderedPairs", "=", 
    RowBox[{"Transpose", "[", 
     RowBox[{"{", 
      RowBox[{"times", ",", "positions"}], "}"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "we", " ", "have", " ", "a", " ", "list", " ", "of", " ", "times", " ", 
    "and", " ", "a", " ", "list", " ", "of", " ", "positions", " ", "and", 
    " ", "we", " ", "need", " ", "ordered", " ", "pairs", " ", "to", " ", 
    "feed", " ", "to", " ", "ListPlot"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"ListPlot", "[", "orderedPairs", "]"}]}], "Input",
 CellChangeTimes->{{3.9463091297316093`*^9, 3.94630915190049*^9}, {
