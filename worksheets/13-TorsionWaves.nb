Cell[TextData[{
 "This angular acceleration formula\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[VeryThinSpace]", 
    RowBox[{
     SubscriptBox["\[Alpha]", "j"], "=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["\[Omega]", "0"], "2"], "(", 
        RowBox[{
         SubscriptBox["\[Theta]", "j"], "-", 
         SubscriptBox["\[Theta]", 
          RowBox[{"j", "-", "1"}]]}], ")"}]}], "+", "\[VeryThinSpace]", 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["\[Omega]", "0"], "2"], "(", 
       RowBox[{
        SubscriptBox["\[Theta]", 
         RowBox[{"j", "+", "1"}]], "-", 
        SubscriptBox["\[Theta]", "j"]}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"1f575398-63ed-43d8-bea9-624bc1c55471"],
