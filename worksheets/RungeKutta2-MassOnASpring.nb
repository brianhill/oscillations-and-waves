Cell[TextData[{
 "Because the force now depends on the position instead of the time, we had \
to revisit our procedure, and the procedure we ended up with is called \
second-order Runge-Kutta:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"930b41f6-011c-4313-85ee-79bb30603a3d"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["x", "*"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"v", "(", 
       SubscriptBox["t", "i"], ")"}], "\[CapitalDelta]", "\[VeryThinSpace]", 
      "t"}]}]}], TraditionalForm]],ExpressionUUID->
  "f9309b99-d13e-40a4-872b-3c96e05ce41f"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"v", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"a", "(", 
         RowBox[{"x", "(", 
          SubscriptBox["t", "i"], ")"}], ")"}], "+", 
        RowBox[{"a", "(", 
         RowBox[{
          SuperscriptBox["x", "*"], "(", 
          SubscriptBox["t", 
           RowBox[{"i", "+", "1"}]], ")"}], ")"}]}], ")"}], "\[CenterDot]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"14d466ca-a010-4a05-9767-324e476f66ff"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"v", "(", 
         SubscriptBox["t", "i"], ")"}], "+", 
        RowBox[{"v", "(", 
         SubscriptBox["t", 
          RowBox[{"i", "+", "1"}]], ")"}]}], ")"}], "\[CenterDot]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"048609ce-f378-4aa2-a78f-05a7c494584d"]
}], "Text",
