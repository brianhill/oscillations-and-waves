Cell[TextData[{
 "For the sinusoidal acceleration notebook, the theory we implemented updated \
all three of time, position, and velocity as follows:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"217c763a-073a-4242-8bbe-a0fc16edc4b6"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"v", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"a", "(", 
        RowBox[{
         SubscriptBox["t", "i"], "+", 
         FractionBox[
          RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}], ")"}],
        "\[CenterDot]", "\[CapitalDelta]"}], "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"84dc0144-0af2-415c-85d8-c2cb009ab008"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"v", "(", 
         SubscriptBox["t", "i"], ")"}], "+", 
        RowBox[{"v", "(", 
         SubscriptBox["t", 
          RowBox[{"i", "+", "1"}]], ")"}]}], ")"}], "\[CenterDot]", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}], "2"]}]}]}], 
