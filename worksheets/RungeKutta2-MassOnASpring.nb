  RowBox[{"procedure", "[", "cc_", "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Extract", " ", "time"}], ",", " ", "position", ",", " ", 
     RowBox[{"and", " ", "velocity", " ", "from", " ", "the", " ", 
      RowBox[{"list", "."}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"curTime", "=", 
     RowBox[{"cc", "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"curPosition", "=", 
     RowBox[{"cc", "[", 
      RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
