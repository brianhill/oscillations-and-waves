  FormBox[
   RowBox[{
    RowBox[{
    "j", " ", "goes", " ", "from", " ", "1", " ", "to", " ", Cell[
     "n",ExpressionUUID->"147f94d8-c42f-4fbf-aa4c-cf83232d8d19"]}], ")"}], 
   TraditionalForm]],ExpressionUUID->"508e44ad-8871-4535-973d-245fb5df3f1a"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["v", "j"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["v", "j"], "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
