     RowBox[{
      RowBox[{
       RowBox[{"Derivative", "[", "2", "]"}], "[", "\[Psi]", "]"}], "[", "x", 
      "]"}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], "k", "\[VeryThinSpace]", 
