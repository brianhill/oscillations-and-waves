Cell[TextData[{
 "Now there are enough things going on that you have to start keeping your \
wits about you. Let\[CloseCurlyQuote]s back off on the complexity for a \
minute by going back to Euler\[CloseCurlyQuote]s method. After we nail that, \
we\[CloseCurlyQuote]ll get back to second-order Runge-Kutta.\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"4308371c-cc0f-477e-a159-c15c003a3920"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["x", "j"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "j"], "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["v", "j"], "(", 
        SubscriptBox["t", "i"], ")"}], "\[CenterDot]", "\[VeryThinSpace]", 
       "\[CapitalDelta]"}], "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"5980590c-d132-4ecc-ac78-e664286b9f9a"],
 "       (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
    "j", " ", "goes", " ", "from", " ", "1", " ", "to", " ", Cell[
     "n",ExpressionUUID->"a6894571-7dae-4cf5-822a-4b2cd3f80d70"]}], ")"}], 
   TraditionalForm]],ExpressionUUID->"a712f601-27cc-40a4-9aa2-e360694a799b"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["v", "j"], "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["v", "j"], "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["a", "j"], "(", 
        RowBox[{
         RowBox[{
         "that", " ", "depends", " ", "on", " ", "neighboring", " ", "x"}], 
         " ", "\[LongDash]", " ", 
         RowBox[{
         "see", " ", "formulas", " ", "for", " ", "the", " ", "accelerations",
           " ", "above"}]}], ")"}], "\[CenterDot]", "\[CapitalDelta]"}], "\[VeryThinSpace]",
       "t"}]}]}], TraditionalForm]],ExpressionUUID->
  "a001b317-c013-42e4-9a61-062a3dcafc19"]
}], "Text",
