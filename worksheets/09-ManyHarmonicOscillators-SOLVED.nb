Cell[BoxData[
 RowBox[{
  RowBox[{"acceleration", "[", 
   RowBox[{"j_", ",", "allPositions_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"-", 
     SuperscriptBox["omega0", "2"]}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"allPositions", "[", 
       RowBox[{"[", "j", "]"}], "]"}], "-", 
      RowBox[{"If", "[", 
