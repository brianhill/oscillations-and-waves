           RowBox[{"n\[Theta]", "-", "1"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"newvs", "=", 
       RowBox[{"curvs", "+", 
        RowBox[{"as", " ", "deltaT"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"newzs", "=", 
       RowBox[{"curzs", "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"curvs", "+", "newvs"}], ")"}], 
