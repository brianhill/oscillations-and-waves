Cell[TextData[{
 "The formula for the accelerations,\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[VeryThinSpace]", 
     RowBox[{
      SubscriptBox["a", 
       RowBox[{"r", ",", "\[Theta]"}]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{
         SubscriptBox["n", "\[Theta]"], " ", 
         SubscriptBox["n", "r"]}], 
        RowBox[{"2", "\[Pi]", "\[VeryThinSpace]", "r"}]], 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["v", "0"], "2"], "[", 
        RowBox[{
         RowBox[{
          FractionBox[
           RowBox[{"2", "\[Pi]", "\[VeryThinSpace]", "r"}], 
           SubscriptBox["n", "r"]], 
          SuperscriptBox[
           RowBox[{"(", 
            FractionBox[
             SubscriptBox["n", "\[Theta]"], 
             RowBox[{"2", "\[Pi]", "\[VeryThinSpace]", "r", " ", 
              SubscriptBox["n", "r"]}]], ")"}], "2"], 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["z", 
             RowBox[{"j", ",", 
              RowBox[{"\[Theta]", "+", "1"}]}]], "+", 
            SubscriptBox["z", 
             RowBox[{"j", ",", 
              RowBox[{"\[Theta]", "-", "1"}]}]], "-", 
            RowBox[{"2", 
             SubscriptBox["z", 
              RowBox[{"r", ",", "\[Theta]"}]]}]}], ")"}]}], "+", 
         RowBox[{
          FractionBox[
           RowBox[{"2", "\[Pi]", "\[VeryThinSpace]", "r"}], 
           SubscriptBox["n", "\[Theta]"]], 
          FractionBox["1", 
           SuperscriptBox[
            SubscriptBox["n", "r"], "2"]], 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["z", 
             RowBox[{
              RowBox[{"r", "+", "1"}], ",", "\[Theta]"}]], "+", 
            SubscriptBox["z", 
             RowBox[{
              RowBox[{"r", "-", "1"}], ",", "\[Theta]"}]], "-", 
            RowBox[{"2", 
             SubscriptBox["z", 
              RowBox[{"r", ",", "\[Theta]"}]]}]}], ")"}]}]}]}]}]}], ")"}], 
    "]"}], TraditionalForm]],ExpressionUUID->
  "1f575398-63ed-43d8-bea9-624bc1c55471"],
 "\n\nis valid except for the perimeter and the center."
}], "Text",
