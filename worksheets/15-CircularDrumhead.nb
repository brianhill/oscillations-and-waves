     RowBox[{
      RowBox[{"curTime", "=", 
       RowBox[{"cc", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"curzs", "=", 
       RowBox[{"cc", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"curvs", "=", 
       RowBox[{"cc", "[", 
        RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"newTime", "=", 
       RowBox[{"curTime", "+", "deltaT"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"zsStar", "=", 
       RowBox[{"curzs", "+", 
