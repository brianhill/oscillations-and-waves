Cell[TextData[{
 "This is a more general version of Second-Order Runge-Kutta, which has a \
parameter \[Alpha], typically chosen as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    FractionBox["1", "2"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "42746172-b599-4703-a2e2-693786be0d3a"],
 " or ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", "1"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9cb7cca9-4f08-4272-97de-7813b20b02ef"],
 ":\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["t", "*"], "=", 
    RowBox[{"t", "+", 
     RowBox[{
     "\[Alpha]", "\[VeryThinSpace]", "\[CapitalDelta]", "\[VeryThinSpace]", 
      "t"}]}]}], TraditionalForm]],ExpressionUUID->
  "1e39087b-909c-4302-b14a-3cdf85fba0c1"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "*"], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"v", "(", 
        SubscriptBox["t", "i"], ")"}], "\[CenterDot]", "\[Alpha]"}], "\[VeryThinSpace]",
       "\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"7ac4f9a1-c270-4b05-aab3-f3b52598fbbc"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["v", "*"], "=", 
    RowBox[{
     RowBox[{"v", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"a", "(", 
        RowBox[{
         SubscriptBox["t", "i"], ",", 
         RowBox[{"x", "(", 
          SubscriptBox["t", "i"], ")"}], ",", 
         RowBox[{"v", "(", 
          SubscriptBox["t", "i"], ")"}]}], ")"}], "\[CenterDot]", 
       "\[Alpha]"}], "\[VeryThinSpace]", "\[CapitalDelta]", "\[VeryThinSpace]",
       "t"}]}]}], TraditionalForm]],ExpressionUUID->
  "19b18ac5-9d92-4984-843a-11dd8d4cafce"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["t", "i"], "+", 
     RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "t"}]}]}], 
   TraditionalForm]],ExpressionUUID->"3adef170-2f17-4efd-b999-05ffb93b6b91"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"v", "(", 
      SubscriptBox["t", "i"], ")"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", 
            FractionBox["1", 
             RowBox[{"2", "\[Alpha]"}]]}], ")"}], 
          RowBox[{"a", "(", 
           RowBox[{
            SubscriptBox["t", "i"], ",", 
            RowBox[{"x", "(", 
             SubscriptBox["t", "i"], ")"}], ",", 
            RowBox[{"v", "(", 
             SubscriptBox["t", "i"], ")"}]}], ")"}]}], "+", 
         RowBox[{
          FractionBox["1", 
           RowBox[{"2", "\[Alpha]"}]], 
          RowBox[{"a", "(", 
           RowBox[{
            SuperscriptBox["t", "*"], ",", 
            SuperscriptBox["x", "*"], ",", 
            SuperscriptBox["v", "*"]}], ")"}]}]}], ")"}], "\[CenterDot]", 
       "\[CapitalDelta]"}], "\[VeryThinSpace]", "t"}]}]}], TraditionalForm]],
  ExpressionUUID->"e69894a2-5db0-4159-8dfb-710077fcf929"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", 
      RowBox[{"i", "+", "1"}]], ")"}], "=", 
    RowBox[{
     RowBox[{"x", "(", 
      SubscriptBox["t", "i"], ")"}], "+", " ", 
     RowBox[{
      FormBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"v", "(", 
          SubscriptBox["t", "i"], ")"}], "+", 
