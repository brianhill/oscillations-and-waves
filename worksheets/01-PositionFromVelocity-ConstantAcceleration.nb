Cell[BoxData[{
 RowBox[{
  RowBox[{"times", " ", "=", " ", 
   RowBox[{"Range", "[", 
    RowBox[{"tInitial", ",", "tFinal", ",", "deltaT"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
